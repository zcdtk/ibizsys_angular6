{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/spec",
    "types": [
      "jasmine",
      "node"
    ],
    "paths": {
      "@shared": [
        "app/shared"
      ],
      "@shared/*": [
        "app/shared/*"
      ],
      "@core": [
        "app/core"
      ],
      "@core/*": [
        "app/core/*"
      ],
      "@testing": [
        "testing"
      ],
      "@testing/*": [
        "testing/*"
      ],
      "@env": [
        "environments"
      ],
      "@env/*": [
        "environments/*"
      ],
      "@pages": [
        "app/pages"
      ],
      "@pages/*": [
        "app/pages/*"
      ],
      "@user": [
        "app/user"
      ],
      "@user/*": [
        "app/user/*"
      ],
      "ibizsys": [
        "app/ibizsys"
      ]
    }
  },
  "files": [
    "test.ts",
    "polyfills.ts"
  ],
  "include": [
    "**/*.spec.ts",
    "**/*.d.ts"
  ]
}
